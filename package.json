{
	"name": "ranobedb",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"db-gen": "kysely-codegen --dialect postgres --schema public --include-pattern public.* --out-file ./sql/codegen/db.d.ts"
	},
	"devDependencies": {
		"@lucia-auth/adapter-postgresql": "^3.1.0",
		"@melt-ui/pp": "^0.3.0",
		"@melt-ui/svelte": "^0.74.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/pg": "^8.10.9",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"deep-object-diff": "^1.1.9",
		"dotenv": "^16.3.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"kysely": "^0.26.3",
		"kysely-codegen": "^0.11.0",
		"lucia": "^3.0.1",
		"pg": "^8.11.3",
		"postcss": "^8.4.31",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"simple-markdown": "^0.7.3",
		"svelte": "^4.0.5",
		"svelte-check": "^3.6.0",
		"svelte-sequential-preprocessor": "^2.0.1",
		"sveltekit-flash-message": "^2.4.2",
		"sveltekit-superforms": "^2.6.1",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"vitest": "^1.0.0",
		"zod": "^3.22.4"
	},
	"dependencies": {
		"oslo": "^1.1.1"
	},
	"type": "module"
}
